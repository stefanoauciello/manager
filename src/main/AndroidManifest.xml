<?xml version="1.0" encoding="utf-8"?><!--suppress ALL -->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:versionCode="2"
    android:versionName="1.1">

    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />
    <application
        android:allowBackup="false"
        android:debuggable="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        tools:ignore="HardcodedDebugMode">
        <activity
            android:name="manager.app.MainActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="manager.app.InsertPlayer"
            android:label="@string/title_activity_insert_player"></activity>
        <activity
            android:name="manager.app.ManagePlayer"
            android:label="@string/title_activity_manage_player"></activity>
        <activity
            android:name="manager.app.FirstFrame"
            android:label="@string/first_frame"></activity>
        <activity
            android:name="manager.app.SelectPlayer"
            android:label="@string/select_player"></activity>
        <activity
            android:name="manager.app.EditPlayer"
            android:label="@string/edit_player"></activity>
        <activity
            android:name="manager.app.OrganizeTeam"
            android:label="@string/organize_team"></activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />
</manifest>
